## The function is currently defined as
function (p, n, K) 
{
    K <- K[K > 0]
    if (p[2] >= 0) {
        range <- c(c(1:n))
        frq <- freq(K, range)
        filter <- frq > 0
        ml <- log(dlnbinom.zt(range[filter], n, p[1], p[2]))
        nloglik <- (-sum(ml * frq[filter]))
        return(nloglik)
    }
    else {
        return(-Inf)
    }
  }
}
