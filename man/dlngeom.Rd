function (k, m, s) 
{
    f <- function(P, k, m, s) dgeom(k, P) * dlogitnorm(P, m, 
        s)
    F <- function(k, m, s) integrate(f, 0, 1, k, m, s)$value
    mapply(F, k, m, s)
  }
}
